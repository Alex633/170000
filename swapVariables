using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace _170000Courses
{
    internal class Program
    {
        static void Main(string[] args)
        {
            string name = "cherepaha";
            string lastName = "cherepahova";
            string container;

            Console.WriteLine($"Your name is {name} \n"+
                $"Your last Name {lastName} \n");

            container = name;
            name = lastName;
            lastName = container;

            Console.WriteLine($"Your name is {name} \n" +
                $"Your last Name {lastName} \n");


        }
    }
}
